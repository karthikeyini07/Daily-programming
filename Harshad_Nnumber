import java.util.*;
class k
{
  public static void main(String[] args)
  {
    Scanner obj=new Scanner(System.in);
    long n=obj.nextLong();
    long num;
    long sum=0;
    long temp=n;
    while(temp!=0)
    {
      num=temp%10;
      sum+=num;
      temp/=10;
    }
    if(n%sum==0)
    {
      System.out.print("Yes");
    }else
    {
      System.out.print("No");
    }
  }
}
